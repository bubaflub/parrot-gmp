$load 'src/GMP/Integer.pbc';

namespace GMP {
  class Integer;
}

function main[main](var argv) {
  var x = new GMP.Integer();
  mpz_set_ui(x, 30);
  mpz_add_ui(x, x, 2);
  say(x);

  var y = new GMP.Integer(32);
  mpz_add_ui(y, y, 2);
  say(y);

  var z = new GMP.Integer("302");
  mpz_add_ui(z, z, 40);
  say(z);
}
